# Production Environment Variables
# Copy this to .env.production and update with your live server details

# API URL - YOUR LIVE BACKEND URL
REACT_APP_API_URL=https://praashibysupal.com/api

# OR if backend is on different domain:
# REACT_APP_API_URL=https://api.praashibysupal.com/api

# Application Settings
REACT_APP_NAME=Praashibysupal
REACT_APP_ENV=production

# Razorpay (if using)
REACT_APP_RAZORPAY_KEY_ID=your_production_razorpay_key_id

# Analytics (optional)
# REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X
