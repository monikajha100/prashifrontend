# Production Environment Configuration
# Copy this file to .env.production and update with your live server settings

# CRITICAL: Set your live backend API URL
REACT_APP_API_URL=https://api.praashibysupal.com/api

# If your backend is on a different domain/subdomain:
# REACT_APP_API_URL=https://api.praashibysupal.com/api

# Application Settings
REACT_APP_NAME=Praashibysupal
REACT_APP_ENV=production

# Disable source maps in production (optional - improves performance)
GENERATE_SOURCEMAP=false